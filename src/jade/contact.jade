doctype html
html
 html
  include ./includes/head.jade
  body    
    include ./includes/overlay.jade
    include ./includes/header.jade 
    main(id='main-contact')
        div(class='container')  
            div(class='row')
                div(class='col-6')
                    h1 Contact us
                    p Fill out this easy form and we`ll get back to you right away.<br />We don`t need much to get you happy!
                    form
                        input(class='contact_form name_form' type="text" name="name" placeholder="NAME")
                        input(class='contact_form name_form' type="text" name="latname" placeholder="LASTNAME")
                        input(class='contact_form' type="text" name="email" placeholder="E-MAIL ADRESS")
                        select(class='contact_form' name="country" )
                          option(value="0") Select country
                          option(value="1") Canada
                          option(value="2") USA
                          option(value="3") Poland
                          option(value="4") England
                          option(value="5") Germany
                        
                        input(class='contact_form message_form' type="text" name="message" placeholder="YOUR MESSAGE GOES HERE...")
                        input(class='contact_submit' type="submit" name="submit" value="SEND MESSAGE" id="dajID")
                div(class='col-6')
                    div(class='map-container')
                         iframe(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2434.2962940422494!2d16.96154281605179!3d52.401306079791944!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47045b0ba78dbcb1%3A0xef2e350f903f43a6!2sMalta+Office+Park!5e0!3m2!1spl!2spl!4v1527941819159" width="600" height="400" frameborder="0" style="border:0" allowfullscreen>)
    include ./includes/footer.jade